  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --production",
    "webpack:build": "webpack",
    "webpack:watch": "webpack --watch",
    "tsc:build": "tsc",
    "tsc:watch": "tsc --watch",
    "test": "jest --maxWorkers=1 --detectOpenHandles",
    "test:watch": "jest --watch --maxWorkers=1",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "postinstall": "npm run license-check",
    "license-check": "itonics-license-checker hosted --prod",
    "migrate": "progresql --config db/migration-local-config.json --migrate --password tests",
    "restdoc": "./.bin/doc || true",
    "generateTemplate": "node mjml-to-handlerbars-template-converter.js --path=/terraform/notification-dispatcher/templates",
    "audit": "better-npm-audit audit --production"
  },
  "devDependencies": {
    "@itonics/license-checker": "^1.2.2",
