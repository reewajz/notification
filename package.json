    "@types/moxios": "^0.4.10",
    "@types/node": "^11.13.4",
    "apidoc": "^0.17.6",
    "apidoc-markdown": "^5.1.5",
    "better-npm-audit": "^3.7.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "jest": "^26.1.0",
    "jest-raw-loader": "^1.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "mjml": "^4.13.0",
    "moxios": "^0.4.0",
